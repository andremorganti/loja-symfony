{% extends 'base.html.twig' %}

{% block title %}Cnae{% endblock %}

{% block body %}
    <h1>Cnae</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cnae.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ cnae.name }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ cnae.code }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_cnae_index') }}" class="btn btn-secondary">Voltar</a>

    <a href="{{ path('app_cnae_edit', {'id': cnae.id}) }}" class="btn btn-warning">Alterar</a>

    {{ include('cnae/_delete_form.html.twig') }}
{% endblock %}
